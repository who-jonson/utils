{
  "name": "@whoj/utils-monorepo",
  "description": "Collection of common JS / TS helpers/utils by @whoj",
  "version": "0.0.6",
  "private": true,
  "license": "MIT",
  "keywords": [
    "typescript",
    "ts",
    "starter"
  ],
  "scripts": {
    "lint": "eslint \"packages/**/*.{js,ts,tsx,vue}\" --fix",
    "prepare": "pnpm -r run stub",
    "build": "pnpm -r run build",
    "play": "vite --config ./playground/vite.config.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "release": "bumpp package.json packages/*/package.json --commit --push --tag && pnpm -r publish --access public"
  },
  "devDependencies": {
    "@antfu/eslint-config": "0.25.2",
    "@types/node": "18.6.5",
    "@testing-library/vue": "6.6.1",
    "@types/throttle-debounce": "5.0.0",
    "@vitejs/plugin-vue": "3.0.1",
    "@vitest/ui": "0.21.0",
    "@whoj/utils": "workspace:^",
    "bumpp": "8.2.1",
    "c8": "7.12.0",
    "core-js": "3.24.1",
    "cross-env": "7.0.3",
    "dot-prop": "7.2.0",
    "eslint": "8.21.0",
    "eslint-plugin-testing-library": "5.6.0",
    "eslint-plugin-tsdoc": "0.2.16",
    "esno": "0.16.3",
    "fast-glob": "3.2.11",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "throttle-debounce": "5.0.0",
    "ts-toolbelt": "9.6.0",
    "type-fest": "2.18.0",
    "typescript": "4.7.4",
    "unbuild": "0.7.6",
    "utility-types": "3.10.0",
    "vite": "3.0.4",
    "vite-dts": "1.0.4",
    "vitest": "0.21.0",
    "vue": "3.2.37"
  },
  "packageManager": "pnpm@7.9.0",
  "author": {
    "name": "Jonson B.",
    "email": "www.jbc.bd@gmail.com",
    "url": "https://who-jonson.github.io"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "type": "module"
}
